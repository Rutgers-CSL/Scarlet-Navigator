{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "dot-notation": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "import/extensions": [2, { "caseSensitive": false }],
    "no-unused-vars": "off",
    "object-curly-newline": "off"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
